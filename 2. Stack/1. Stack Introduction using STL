#include<iostream>
#include<stack>
using namespace std;
//Stack Data Structure
int main(){
 
 //Creation of stack
 stack <int> s;

//Push Operation in Stack
 s.push(2);
 s.push(3);
 s.push(4);

//Pop operation Stack
 s.pop();

 /* Top Operation */
 cout<<"Printing top element "<<s.top()<<endl;

 
 if(s.empty())
 cout<<"Stack is Not Empty";

 else
cout<<"Stack is not empty";

cout<<endl<<"Size of stack is "<<s.size();
}
