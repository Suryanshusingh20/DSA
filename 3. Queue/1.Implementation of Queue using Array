#include<iostream>
#include<queue>
using namespace std;
//Queue Using STL
int main(){
		//Create a Queue
		queue<int> q;
		
		//Push Operation	
		q.push(11);
		q.push(12);
		q.push(13);
		q.push(14);
		q.push(15);
		
		//Size of Queue
		cout<<"Size of Quese is "<<q.size()<<endl;
		
		//Pop Operation
		q.pop();
		q.pop();
		
		//Size of Queue
	  	cout<<"Size of Quese is "<<q.size()<<endl;
	  	cout<<"Front of queue is "<<q.front()<<endl;
	  	
	  	//Empty Operation(Bool)
	  	if(q.empty()){
	  		cout<<"Queue is Empty"<<endl;
		  }
		else{
			cout<<"Queue is not empty"<<endl;
			
		}
		
		return 0;	
}
